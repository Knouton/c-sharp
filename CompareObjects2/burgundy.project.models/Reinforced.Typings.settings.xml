<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	
	
	<PropertyGroup>
	
    <RtTargetFile>$(ProjectDir)typings\src\Server1.ts</RtTargetFile>
		
		<RtConfigurationMethod></RtConfigurationMethod>
		<RtWriteWarningComment >false</RtWriteWarningComment >

		<RtDivideTypesAmongFiles>true</RtDivideTypesAmongFiles>
		<RtTargetDirectory>$(ProjectDir)typings\src</RtTargetDirectory>
		<RtBypassTypeScriptCompilation>false</RtBypassTypeScriptCompilation>				
		<RtDisable>false</RtDisable>
		<RtForceTargetFramework>netcoreapp3.0</RtForceTargetFramework>
		<RtCamelCaseForMethods>true</RtCamelCaseForMethods>
		<RtCamelCaseForProperties>true</RtCamelCaseForProperties>

		<RtConfigurationMethod>Burgundy.Project.Models.typings.TypingsConfiguration.ConfigureTypings</RtConfigurationMethod>


	</PropertyGroup>



	<ItemGroup>
		<RtAdditionalAssembly Include="Burgundy.Shared.Models"/>
		<RtAdditionalAssembly Include="Burgundy.Tag.Models"/>
		<RtAdditionalAssembly Include="Burgundy.Longread.Models"/>
		</ItemGroup>


